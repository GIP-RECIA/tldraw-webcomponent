{
  "name": "@gip-recia/tldraw-webcomponent",
  "version": "0.1.3",
  "type": "module",
  "files": [
    "dist/*.js",
    "dist/assets"
  ],
  "description": "tldraw web component",
  "keywords": [
    "tldraw",
    "web component",
    "gip-recia"
